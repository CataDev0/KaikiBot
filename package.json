{
	"name": "kaikibot",
	"version": "6.3.0",
	"main": "./dist/index.js",
	"type": "commonjs",
	"scripts": {
		"build": "rm -rf dist/ && npx prisma migrate deploy && npx prisma generate && tsc -p tsconfig.json",
		"dev": "npm run watch & npm run start",
		"format": "prettier --write .",
		"lint": "eslint .",
		"start": "node dist/index.js --color",
		"version": "auto-changelog -p -u -o CHANGELOG.md -t keepachangelog && git add CHANGELOG.md",
		"watch": "tsc -p tsconfig.json -w",
		"deploy": "npm ci --include=dev && npm run build && npm prune --omit=dev"
	},
	"keywords": [
		"Community",
		"Customizable",
		"Discord",
		"Typescript",
		"Prisma",
		"MySQL",
		"Multipurpose",
		"Docker"
	],
	"author": {
		"name": "Cata",
		"email": "cata@cata.moe"
	},
	"license": "MIT",
	"dependencies": {
		"@discordjs/voice": "^0.19.0",
		"@prisma/client": "^5.22.0",
		"@sapphire/decorators": "^6.1.1",
		"@sapphire/framework": "^5.3.6",
		"@sapphire/plugin-logger": "^4.0.2",
		"@sapphire/plugin-subcommands": "^7.0.1",
		"@sapphire/shapeshift": "^4.0.0",
		"@snazzah/davey": "^0.1.7",
		"bufferutil": "^4.0.8",
		"discord-js-button-pagination-ts": "^4.2.2-1",
		"discord.js": "^14.20.0",
		"dotenv": "^16.4.7",
		"express": "^4.21.0",
		"mysql2": "^3.11.5",
		"opusscript": "^0.0.8",
		"prisma": "^5.22.0",
		"sharp": "^0.34.2",
		"source-map-support": "^0.5.21",
		"utf-8-validate": "^6.0.5"
	},
	"devDependencies": {
		"@types/express": "^5.0.3",
		"@typescript-eslint/eslint-plugin": "^8.43.0",
		"@typescript-eslint/parser": "^8.43.0",
		"auto-changelog": "^2.5.0",
		"eslint": "^9.35.0",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"kaikiwa-types": "./KaikiWA-Types/",
		"typescript": "~5.4.5"
	},
	"description": "Discord bot built with Sapphire, Prisma and MySQL2, written in TypeScript.",
	"repository": {
		"type": "git",
		"url": "https://github.com/CataDev0/KaikiBot.git"
	},
	"homepage": "https://github.com/CataDev0/KaikiBot#readme",
	"bugs": {
		"url": "https://github.com/CataDev0/KaikiBot/issues"
	},
	"engines": {
		"node": ">=18.17.0",
		"npm": ">=9.0.0"
	},
	"overrides": {
		"@emnapi/runtime": "0.0.0"
	}
}
